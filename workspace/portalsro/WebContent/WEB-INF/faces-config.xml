<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<message-bundle>resources.pages</message-bundle>
	</application>

	<managed-bean>
		<managed-bean-name>selectTipoDeListas</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.ui.SelectItemsTipoDeLista</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>selectDrs</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.ui.SelectItemsDR</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
        <managed-bean-name>selectTipoEstacao</managed-bean-name>
        <managed-bean-class>br.com.correios.sromonitor.ui.SelectItemsTipoEstacao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
	<managed-bean>
		<managed-bean-name>dataHora</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.CalendarioManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>selectTipoUnidade</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.ui.SelectItemsTipoUnidade</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ultimaTransmissao</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.UltimaTransmissaoManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>semTransmissao</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.SemTransmissaoManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>controleTransmissao</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.ControleTransmissaoManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pendenciaBaixa</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.PendenciaBaixaManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pendenciaBaixa2</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.PendenciaBaixaManagedBean2</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>estacoesDeTrabalho</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.EstacoesDeTrabalhoManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>webDifusao</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.DifusaoWebSROIIWebSphereManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>arquivosRepetidos</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.ArquivoRepetidoManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>objetoIrregular</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.ObjetoIrregularManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>importar</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.dao.lerArquivo</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cadastroConcentradorSro</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.ConcentradorManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>detalhePendencia</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.DetalhePendenciaBaixaManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>codigo</property-name>
			<value>#{param.codigo}</value>
		</managed-property>
		<managed-property>
			<property-name>nome</property-name>
			<value>#{param.nome}</value>
		</managed-property>
		<managed-property>
			<property-name>tipoDeLista</property-name>
			<value>#{param.tipoDeLista}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>detalheEstacoesDeTrabalho</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.DetalheEstacoesDeTrabalhoManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>codigo</property-name>
			<value>#{param.codigo}</value>
		</managed-property>
		<managed-property>
			<property-name>nome</property-name>
			<value>#{param.nome}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Events</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.ui.Events</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>faixaCep</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.FaixaCepManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pendenciaBaixaDiaria</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.PendenciaBaixaDiariaManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>conferenciaLeocBean</managed-bean-name>
		<managed-bean-class>br.com.correios.sromonitor.managed.beans.ConferenciaLeocBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/ultimaTransmissao.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultUltimaTransmissao.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/semTransmissao.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultSemTransmissao.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/editorConcentrador.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/cadastroConcentradoresSROII.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/controleTransmissao.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultControleTransmissao.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pendenciaBaixa.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultPendenciaBaixa.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/estacoesDeTrabalho.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultEstacoesDeTrabalho.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/DifusaoWebSROII.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultDifusaoWebSROII.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/ConferenciaLeoc.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>resultConferenciaLeoc.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pendenciaBaixa2.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultPendenciaBaixa2.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/resultPendenciaBaixa.jsp</from-view-id>
		<navigation-case>
			<from-outcome>detalhe</from-outcome>
			<to-view-id>/detalhePendenciaBaixa.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/resultEstacoesDeTrabalho.jsp</from-view-id>
		<navigation-case>
			<from-outcome>detalhe</from-outcome>
			<to-view-id>/detalheEstacoesDeTrabalho.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>/arquivosRepetidos.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultArquivosRepetidos.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/objetosIrregulares.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultObjetoIrregular.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- Faixa de CEP -->
	<navigation-rule>
		<from-view-id>/faixaCep.jsp</from-view-id>
		<navigation-case>
			<from-outcome>inclusao</from-outcome>
			<to-view-id>/incluirFaixaCep.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>exclusao</from-outcome>
			<to-view-id>/excluirFaixaCep.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/resultFaixaCep.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/faixaCep.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/incluirFaixaCep.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/faixaCep.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/incluirFaixaCep.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/excluirFaixaCep.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/faixaCep.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/excluirFaixaCep.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/error.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
