package br.com.correios.sromonitor.vos;

import java.io.Serializable;
import java.util.Date;

public class FaixaCepUnidadeOperacional implements Serializable {
	
	private static final long serialVersionUID = 1L;
	
	private Long id;
	private String codigoUnidade;
	private String codigoMcu;
	private String nomeUnidade;
	private String cepInicial;
	private String cepFinal;
	private Byte tipoUnidade;
	private Date dataHoraAtualizacao;

	
	public FaixaCepUnidadeOperacional(Long id, String codigoUnidade, String nomeUnidade, String cepInicial, String cepFinal, Byte tipoUnidade, Date dataHoraAtualizacao) {
		this.id = id;
		this.codigoUnidade = codigoUnidade;
		this.nomeUnidade = nomeUnidade;
		this.cepInicial = cepInicial;
		this.cepFinal = cepFinal;
		this.tipoUnidade = tipoUnidade;
		this.dataHoraAtualizacao = dataHoraAtualizacao;
		
		
	}
	
	public FaixaCepUnidadeOperacional(Long id, String codigoUnidade, String codigoMcu, 
			String nomeUnidade, String cepInicial, String cepFinal, Byte tipoUnidade, Date dataHoraAtualizacao) {
		this.id = id;
		this.codigoUnidade = codigoUnidade;
		this.codigoMcu = codigoMcu;
		this.nomeUnidade = nomeUnidade;
		this.cepInicial = cepInicial;
		this.cepFinal = cepFinal;
		this.tipoUnidade = tipoUnidade;
		this.dataHoraAtualizacao = dataHoraAtualizacao;
		
		
	}



	
	public Long getId() {
		return this.id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getCodigoUnidade() {
		return this.codigoUnidade;
	}
	
	public void setCodigoUnidade(String codigoUnidade) {
		this.codigoUnidade = codigoUnidade;
	}



	public String getCodigoMcu() {
		return codigoMcu;
	}


	public void setCodigoMcu(String codigoMcu) {
		this.codigoMcu = codigoMcu;
	}




	public String getNomeUnidade() {
		return nomeUnidade;
	}

	public void setNomeUnidade(String nomeUnidade) {
		this.nomeUnidade = nomeUnidade;
	}

	public String getCepInicial() {
		return this.cepInicial;
	}

	public void setCepInicial(String cepInicial) {
		this.cepInicial = cepInicial;
	}

	public String getCepFinal() {
		return this.cepFinal;
	}

	public void setCepFinal(String cepFinal) {
		this.cepFinal = cepFinal;
	}

	public Byte getTipoUnidade() {
		return this.tipoUnidade;
	}

	public void setTipoUnidade(Byte tipoUnidade) {
		this.tipoUnidade = tipoUnidade;
	}

	public Date getDataHoraAtualizacao() {
		return this.dataHoraAtualizacao;
	}

	public void setDataHoraAtualizacao(Date dataHoraAtualizacao) {
		this.dataHoraAtualizacao = dataHoraAtualizacao;
	}

}
